; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Save The Day"
run/main_scene="res://src/screens/game/Game.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

C="*res://src/globals/constants.gd"
SceneManager="*res://src/globals/scene_manager.gd"
WorldManager="*res://src/globals/world_manager.gd"
UIManager="*res://src/globals/ui_manager.gd"
Util="*res://src/globals/util.gd"
Debug="*res://src/globals/debug.gd"
TilePattern="*res://src/globals/tile_pattern.gd"
DialogueManager="*res://src/globals/dialogue_manager.gd"
VfxManager="*res://src/globals/vfx_manager.gd"
MusicManager="*res://src/globals/music_manager.gd"
SfxManager="*res://src/globals/sfx_manager.gd"
SystemManager="*res://src/globals/system_manager.gd"
AnimationManager="*res://src/globals/animation_manager.gd"
EntityManager="*res://src/globals/entity_manager.gd"
PlayerManager="*res://src/globals/player_manager.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
"res://src/abilities/": "blue",
"res://src/components/": "gray",
"res://src/entities/": "orange",
"res://src/entities/entity/presets/main_characters/": "green",
"res://src/levels/": "yellow"
}

